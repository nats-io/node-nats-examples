{
  "name": "node-nats-examples",
  "version": "1.0.0",
  "description": "nats javascript documentation examples",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && npm run ava",
    "debugtest": "tsc && node node_modules/.bin/ava --verbose -T 6500000 -m",
    "clean": "rm -Rf build/ node_modules/",
    "lint": "standard './src/*.js'",
    "fmt": "deno fmt ./src",
    "ava": "nyc ava --verbose -T 60000"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//nats-io/node-nats-examples"
  },
  "author": "The NATS Authors",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "^14.14.25",
    "ava": "^5.2.0",
    "nats": "^2.0.1",
    "nyc": "^15.1.0",
    "typescript": "^4.1.3"
  },
  "ava": {
    "failFast": false,
    "files": [
      "./lib/**/*.js",
      "!./lib/helpers/**/*.js"
    ]
  },
  "nyc": {
    "extension": [
      ".ts",
      ".js"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [],
    "sourceMap": true,
    "all": true
  }
}
